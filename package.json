{
  "name": "avify-client",
  "version": "0.0.17",
  "description": "Avify Javascript Client Library",
  "main": "lib/index.js",
  "types": "lib",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "echo No tests specified yet",
    "temp":"node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bananacode-co/avify-npm.git"
  },
  "keywords": [
    "avify",
    "api",
    "oms",
    "lib"
  ],
  "author": "Banancode Inc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bananacode-co/avify-npm/issues"
  },
  "homepage": "https://github.com/bananacode-co/avify-npm#readme",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
